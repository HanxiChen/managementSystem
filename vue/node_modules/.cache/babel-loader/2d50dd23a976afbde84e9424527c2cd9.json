{"ast":null,"code":"export default {\n  name: \"User\",\n\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 2,\n      username: \"\",\n      nickname: \"\",\n      email: \"\",\n      address: \"\",\n      dialogFormVisible: false,\n      form: {},\n      multipleSelection: [],\n      headerBg: 'headerBg'\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      this.request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username,\n          email: this.email,\n          address: this.address\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.records;\n        this.total = res.total;\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      //页面大小切换\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //当前页切换\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    },\n\n    reset() {\n      //重置按钮\n      this.username = \"\";\n      this.email = \"\";\n      this.address = \"\";\n      this.load();\n    },\n\n    handleAdd() {\n      //新增按钮\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      this.request.post(\"user\", this.form).then(res => {\n        //弹窗中的确定按钮\n        if (res) {\n          this.$message.success(\"保存成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n      this.dialogFormVisible = false;\n    },\n\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id);\n      this.request.post(\"/user/del/batch\", ids).then(res => {\n        if (res) {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n\n    handleEdit(row) {\n      //编辑按钮\n      this.form = row;\n      this.dialogFormVisible = true;\n      this.load();\n    },\n\n    handleDel(id) {\n      //删除按钮\n      this.request.delete(\"/user/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n\n    exp() {\n      window.open(\"http://localhost:9090/user/export\");\n    },\n\n    handleExcelImportSuccess() {\n      this.$message.success(\"文件导入成功\");\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkGA;EACAA,YADA;;EAEAC;IACA;MACAC,aADA;MAEAC,QAFA;MAGAC,UAHA;MAIAC,WAJA;MAKAC,YALA;MAMAC,YANA;MAOAC,SAPA;MAQAC,WARA;MASAC,wBATA;MAUAC,QAVA;MAWAC,qBAXA;MAYAC;IAZA;EAcA,CAjBA;;EAkBAC;IACA;EACA,CApBA;;EAqBAC;IACAC;MACA;QACAC;UACAb,qBADA;UAEAC,uBAFA;UAGAC,uBAHA;UAIAE,iBAJA;UAKAC;QALA;MADA,GAQAS,IARA,CAQAC;QACAC;QAEA;QACA;MACA,CAbA;IAcA,CAhBA;;IAiBAC;MAAA;MACAD;MACA;MACA;IACA,CArBA;;IAsBAE;MAAA;MACAF;MACA;MACA;IACA,CA1BA;;IA2BAG;MAAA;MACA;MACA;MACA;MACA;IACA,CAhCA;;IAiCAC;MAAA;MACA;MACA;IACA,CApCA;;IAqCAC;MACA;QAAA;QACA;UACA;UACA;UACA;QACA,CAJA,MAIA;UACA;QACA;MACA,CARA;MASA;IACA,CAhDA;;IAiDAC;MACAN;MACA;IACA,CApDA;;IAqDAO;MACA;MACA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CA/DA;;IAgEAC;MAAA;MACA;MACA;MACA;IACA,CApEA;;IAqEAC;MAAA;MACA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CA9EA;;IA+EAC;MACAC;IACA,CAjFA;;IAkFAC;MACA;MACA;IACA;;EArFA;AArBA","names":["name","data","tableData","total","pageNum","pageSize","username","nickname","email","address","dialogFormVisible","form","multipleSelection","headerBg","created","methods","load","params","then","res","console","handleSizeChange","handleCurrentChange","reset","handleAdd","save","handleSelectionChange","delBatch","handleEdit","handleDel","ex