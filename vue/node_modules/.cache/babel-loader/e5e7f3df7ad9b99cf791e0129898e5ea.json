{"ast":null,"code":"export default {\n  name: \"role\",\n\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 2,\n      name: \"\",\n      menuDialogVis: false,\n      dialogFormVisible: false,\n      form: {},\n      multipleSelection: [],\n      headerBg: 'headerBg',\n      menuData: [{\n        id: 1,\n        label: '主页',\n        children: []\n      }, {\n        id: 2,\n        label: '系统管理',\n        children: [{\n          id: 3,\n          label: '用户管理',\n          children: []\n        }, {\n          id: 4,\n          label: '角色管理',\n          children: []\n        }, {\n          id: 5,\n          label: '菜单管理',\n          children: []\n        }, {\n          id: 6,\n          label: '文件管理',\n          children: []\n        }]\n      }]\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      this.request.get(\"/role/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      //页面大小切换\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //当前页切换\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    },\n\n    reset() {\n      //重置按钮\n      this.name = \"\";\n      this.load();\n    },\n\n    handleAdd() {\n      //新增按钮\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      this.request.post(\"/role\", this.form).then(res => {\n        //弹窗中的确定按钮\n        if (res.code === '200') {\n          this.$message.success(\"保存成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n      this.dialogFormVisible = false;\n    },\n\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id);\n      this.request.post(\"/role/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n\n    handleEdit(row) {\n      //编辑按钮\n      this.form = row;\n      this.dialogFormVisible = true;\n      this.load();\n    },\n\n    handleDel(id) {\n      //删除按钮\n      this.request.delete(\"/role/\" + id).then(res => {\n        if (res.data) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n\n    selectMenu(roleId) {\n      this.menuDialogVis = true;\n    },\n\n    handleCheckChange(data, checked, indeterminate) {\n      console.log(data, checked, indeterminate);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+EA;EACAA,YADA;;EAEAC;IACA;MACAC,aADA;MAEAC,QAFA;MAGAC,UAHA;MAIAC,WAJA;MAKAL,QALA;MAMAM,oBANA;MAOAC,wBAPA;MAQAC,QARA;MASAC,qBATA;MAUAC,oBAVA;MAWAC,WACA;QACAC,KADA;QAEAC,WAFA;QAGAC;MAHA,CADA,EAMA;QACAF,KADA;QAEAC,aAFA;QAGAC;UACAF,KADA;UAEAC,aAFA;UAGAC;QAHA,GAKA;UACAF,KADA;UAEAC,aAFA;UAGAC;QAHA,CALA,EAUA;UACAF,KADA;UAEAC,aAFA;UAGAC;QAHA,CAVA,EAeA;UACAF,KADA;UAEAC,aAFA;UAGAC;QAHA,CAfA;MAHA,CANA;IAXA;EA0CA,CA7CA;;EA8CAC;IACA;EACA,CAhDA;;EAiDAC;IACAC;MACA;QACAC;UACAd,qBADA;UAEAC,uBAFA;UAGAL;QAHA;MADA,GAMAmB,IANA,CAMAC;QACA;QACA;MACA,CATA;IAUA,CAZA;;IAaAC;MAAA;MACAC;MACA;MACA;IACA,CAjBA;;IAkBAC;MAAA;MACAD;MACA;MACA;IACA,CAtBA;;IAuBAE;MAAA;MACA;MACA;IACA,CA1BA;;IA2BAC;MAAA;MACA;MACA;IACA,CA9BA;;IA+BAC;MACA;QAAA;QACA;UACA;UACA;UACA;QACA,CAJA,MAIA;UACA;QACA;MACA,CARA;MASA;IACA,CA1CA;;IA2CAC;MACAL;MACA;IACA,CA9CA;;IA+CAM;MACA;MACA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CA